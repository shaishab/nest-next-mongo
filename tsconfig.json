{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "strict": true,
    "incremental": true,
    "tsBuildInfoFile": "./dist/tsconfig.tsbuildinfo",
    "rootDir": "./src/server",
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "exclude": [".next"],
  "include": ["./src/server/**/*.ts"],
  "files": ["./src/server/main.ts"],
  "references": []
}